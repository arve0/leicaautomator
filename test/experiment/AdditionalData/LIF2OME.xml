<?xml version="1.0" encoding="utf-8"?>
<ExporterStructureTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation=".\LIF2OME.xsd">
  <!--This XML file is a template describing the semantic structure of all files, tokens, separators and-->
  <!--sub folder structures as need for the data organisation for the current MatrixScreener Application-->
  <Manufacturer>Leica Microsystems</Manufacturer>
  <Version>3.4</Version>
  <LastChangeDate>23.08.2013</LastChangeDate>
  <!--added Experimenter tag-->
  <Author>Frank Sieckmann, Erik Hartmann</Author>
  <Experimenter>
    <UserData Institution="Leica Microsystems CMS GmbH" FirstName="Frank" LastName="Sieckmann" OMEName="Frank" Email="Frank.Sieckmann@leica-microsystems.com" />
  </Experimenter>
  <Group>
    <GroupData Contact="Frank.Sieckmann@leica-microsystems.com" Leader="Frank Sieckmann" Name="MatrixScreener Group" />
  </Group>
  <FolderSubStructure>
    <MediaPath>
      <!--The MEDIAPATH folder is the root or just additional subfolder for all experiments. The base media path will specified by the operator in LAS AF-->
      <!--If you define absolute URL (like D:\MyExportFolder), this one will be used (overwrites LAS AF URL setting). -->
      <!--If you define relative URL, just subfolder (like \MyExportFolder) this will be appended to the base URL defined in LAS AF-->
      <MediaPathName>\3.3.0_9757</MediaPathName>
      <MediaBackupPathName>D:\MatrixScreenerImagesAlternative\</MediaBackupPathName>
      <ExperimentFolder>
        <!--The EXPERIMENT folder is the first subfolder on the ..\MediaPath\ location. It will generated by the program-->
        <!--Each new experiment will generate a new experiment subfolder!-->
        <!--..\MediaPath\ExperimentFolder- -TimeStamp\-->
        <ExperimentFolderName>experiment</ExperimentFolderName>
        <TimeStampIsLocaltime>0</TimeStampIsLocaltime>
        <TimeStampFormat>YYYY_MM_DD_hh_mm_ss</TimeStampFormat>
        <MetaDataFolderName>metadata</MetaDataFolderName>
        <LoopFolder>
          <!--The Loop folder is the 2nd subfolder on the ..\MediaPath\ExperimentFolder- -TimeStamp\ location.-->
          <!--It will generated by the program-->
          <!--It is possible to have multiple loop folder (indicated with L01; L02 ...-->
          <!--..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\-->
          <!--if name is left blank, folder will not be used and left out-->
          <LoopFolderName>
          </LoopFolderName>
          <SlideFolder>
            <!--The SLIDE folder is the 3rd subfolder on the ..\MediaPath\ExperimentFolder- -TimeStamp\ location.-->
            <!--It will generated by the program-->
            <!--It is possible to have multiple slide folder (indicated with S01; S02 ...-->
            <!--..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\Slide- -S01\-->
            <!--if name is left blank, folder will not be used and left out-->
            <SlideFolderName>slide</SlideFolderName>
            <VibMicFolder>
              <!--The MECHCUT folder is the 4rd subfolder on the ..\MediaPath\ExperimentFolder- -TimeStamp\Slide- -S01-\ location.-->
              <!--It will generated by the program-->
              <!--It is possible to have multiple MechCut folder (indicated with M01; M02 ...-->
              <!--..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\Slide- -S01\MechCut- -M01\-->
              <!--if name is left blank, folder will not be used and left out-->
              <VibMicFolderName>MechCut</VibMicFolderName>
              <WellFolder>
                <!--The WELL folder is the 5th subfolder on the ..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\Slide- -S01\MechCut- -M01\ location-->
                <!--It will generated by the program-->
                <!--It is possible to have multiple well folder (indicated with well- -U01- -V01; well- -U01- -V02 ...-->
                <!--..\MediaPath\ExperimentFolder- -TimeStamp\Slide- -S01\MechCut- -M01\Well- -U01- -V02\-->
                <!--- - - - - - - - - - - - - - - - - -->
                <!--Instead of well this folder name can be also called chamber (in case we are working with LabTEK slides)-->
                <!--if name is left blank, folder will not be used and left out-->
                <WellFolderName>chamber</WellFolderName>
                <!--The MetaData Folder contains a copy of all experiment relevant meta data (copy of the scanning template,-->
                <!--the IPS, SEQ or other LASAF files).-->
                <FieldFolder>
                  <!--The FIELD folder is the 6th subfolder on the-->
                  <!--..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\Slide- -S01\MechCut- -M01\Well- -U01- -V02\ location-->
                  <!--It will generated by the program-->
                  <!--It is possible to have multiple field folder (indicated with field- -X01- -Y01; field- -X01- -Y02 ...-->
                  <!--..\MediaPath\ExperimentFolder- -TimeStamp\Loop- -L01\Slide- -S01\MechCut- -M01\Well- -U01- -V02\Field- -X01- -Y02\-->
                  <!--if name is left blank, folder will not be used and left out-->
                  <FieldFolderName>field</FieldFolderName>
                </FieldFolder>
                <SubfieldFolder>
                  <!--The Subfield folder is the 7th subfolder on the-->
                  <!--It will generated by the program-->
                  <!--if name is left blank, folder will not be used and left out-->
                  <SubfieldFolderName>Subfield</SubfieldFolderName>
                </SubfieldFolder>
                <AFJobFolder>
                  <!--The AFJob folder is the 7th subfolder on the-->
                  <!--It will generated by the program-->
                  <!--It contains the autofocus and drift compensation images. -->
                  <!--If name is left blank, folder will not be used and left out-->
                  <AFJobFolderName>AF</AFJobFolderName>
                </AFJobFolder>
              </WellFolder>
            </VibMicFolder>
          </SlideFolder>
        </LoopFolder>
      </ExperimentFolder>
    </MediaPath>
  </FolderSubStructure>
  <TokenNames>
    <!--e.g. "image- -L0004- -S02- -M01- -U01- -V01- -J02- -E05- -X01- -Y01- -T0004- -Z01- -C01.tif"-->
    <!--Token ImageName : default = image-->
    <!--Token L : This is the Loop count number. L01 = Loop count 1 (in case of xyz scans L and T will increase simultan; in case of xyzt scans L and t will increase separately)-->
    <!--Token S : This is the slide number. S01 = Slide number 1-->
    <!--Token M : M = mechanical cuts level (reserverd for development) -->
    <!--Token U : This is the Well position in X direction (or chamber position if we use LabTEK Slides)-->
    <!--Token V : This is the Well position in Y direction (or chamber position if we use LabTEK Slides)-->
    <!--Token J:   J = position used for acquring multiple images at  same field position, e.g. applying different jobs (PatternElements) within one field -->
    <!--Token P : P = position will used for subfield, e.g. tiled images within one field-->
    <!--Token O : O = order of job in field, value is reseted to zero everytime microscope moves into a new field (for loops in LDM patterns only!)-->
    <!--Token X : This is the Field position in X direction within any well on position U, V-->
    <!--Token Y : This is the Field position in Y direction within any well on position U, V-->
    <!--Token Z : This is the Slice position within any well on position U, V of any field on position X,Y-->
    <!--Token T : This is the Time position within any well on position U, V of any field on position X,Y,Z (capital T in case of an xyz scan; small t in case of an xyzt scan)-->
    <!--Token C : This is the used channel within any well on position U, V of any field on position X,Y,Z,T-->
    <TokenImageName>image</TokenImageName>
    <TokenLoopCount>L</TokenLoopCount>
    <TokenSlideNumber>S</TokenSlideNumber>
    <TokenVibMicPos>M</TokenVibMicPos>
    <TokenWellXPos>U</TokenWellXPos>
    <TokenWellYPos>V</TokenWellYPos>
    <TokenFieldJPos>J</TokenFieldJPos>
    <TokenFieldPPos>P</TokenFieldPPos>
    <TokenFieldRPos>R</TokenFieldRPos>
    <TokenFieldLamPos>Lamda</TokenFieldLamPos>
    <TokenFieldXTPos>XT</TokenFieldXTPos>
    <TokenFieldTSPos>TS</TokenFieldTSPos>
    <TokenFieldLamExcPos>LamExc</TokenFieldLamExcPos>
    <TokenFieldStagePos>Stage</TokenFieldStagePos>
    <TokenFieldOrderOfJobInField>O</TokenFieldOrderOfJobInField>
    <TokenFieldXPos>X</TokenFieldXPos>
    <TokenFieldYPos>Y</TokenFieldYPos>
    <TokenFieldTPos>T</TokenFieldTPos>
    <TokenFieldZPos>Z</TokenFieldZPos>
    <TokenFieldCPos>C</TokenFieldCPos>
  </TokenNames>
  <Format>
    <!--The separator is currently a double dash-->
    <!--Sample: image- -L0004- -S02- -M01- -P01- -U01- -V01- -J02- -E05- -X01- -Y01- -T0004- -Z01- -C01.tif-->
    <!--Sample for 2 digits = T01-->
    <!--Sample for 3 digits = T001-->
    <!--Sample for 4 digits = T0001 ...-->
    <Separator>--</Separator>
    <FileType>tif</FileType>
    <UseLoopTokenIncForTimeTokenInc>1</UseLoopTokenIncForTimeTokenInc>
    <TokenSlideNumberDigits>2</TokenSlideNumberDigits>
    <TokenVibMicPosDigits>2</TokenVibMicPosDigits>
    <TokenLoopCountDigits>4</TokenLoopCountDigits>
    <TokenWellXPosDigits>2</TokenWellXPosDigits>
    <TokenWellYPosDigits>2</TokenWellYPosDigits>
    <TokenFieldJPosDigits>2</TokenFieldJPosDigits>
    <TokenFieldPPosDigits>2</TokenFieldPPosDigits>
    <TokenFieldRPosDigits>2</TokenFieldRPosDigits>
    <TokenFieldLamPosDigits>2</TokenFieldLamPosDigits>
    <TokenFieldXTPosDigits>2</TokenFieldXTPosDigits>
    <TokenFieldTSPosDigits>2</TokenFieldTSPosDigits>
    <TokenFieldLamExcPosDigits>2</TokenFieldLamExcPosDigits>
    <TokenFieldStagePosDigits>3</TokenFieldStagePosDigits>
    <TokenFieldOrderOfJobInFieldPosDigits>2</TokenFieldOrderOfJobInFieldPosDigits>
    <TokenFieldXPosDigits>2</TokenFieldXPosDigits>
    <TokenFieldYPosDigits>2</TokenFieldYPosDigits>
    <TokenFieldTPosDigits>4</TokenFieldTPosDigits>
    <TokenFieldZPosDigits>2</TokenFieldZPosDigits>
    <TokenFieldCPosDigits>2</TokenFieldCPosDigits>
  </Format>
  <OMEXMLOptions>
    <!-- options "none", "first", "all" -->
    <CustomAttributes>none</CustomAttributes>
    <!-- options "none", "first", "all" -->
    <UUIDGeneration>all</UUIDGeneration>
  </OMEXMLOptions>
</ExporterStructureTemplate>